{
  "name": "online-films-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"./**/*.{ts,tsx,json}\"",
    "lint:es": "eslint --ext .js,.jsx,.ts,.tsx",
    "lint:es:fix": "eslint ./pages ./app --ext .ts,.tsx,.js,.jsx --fix",
    "lint:css:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "prepare": "husky install",
    "lint-staged": "eslint ./pages ./app --ext .ts,.tsx,.js,.jsx --fix && prettier --write \"./**/*.{js,jsx,ts,tsx,md,css,scss,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,json}": [
      "npm run lint-staged",
      "git add"
    ]
  },
  "dependencies": {
    "next": "12.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.57.1"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "autoprefixer": "^10.4.13",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^4.3.8",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.2",
    "stylelint": "^14.16.1",
    "stylelint-config-clean-order": "^2.3.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  }
}
